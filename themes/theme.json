	"tokenColors": [
		{
			"name": "Exports",
			"scope": [
				"meta.declaration.exports.haskell entity.name.function.haskell",
				"meta.declaration.exports.haskell entity.name.function.infix.haskell"
			],
			"settings": {
				"foreground": "#efefef"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"meta.preprocessor.haskell"
			],
			"settings": {
				"foreground": "#666666"
			}
		},
		{
			"name": "Comment Section",
			"scope": [
				"comment.section"
			],
			"settings": {
				"foreground": "#996666",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Comment Quote",
			"scope": [
				"comment.quote"
			],
			"settings": {
				"foreground": "#b5b5b5"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant.numeric",
				"string"
			],
			"settings": {
				"foreground": "#88A04A"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"support.type.property-name.json",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#6699CC"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#B14744"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"entity.name.namespace",
				"meta.import.qualifier.haskell",
				"constant.other.haskell",
				"entity.other.inherited-class.haskell",
				"support.constant.unit.haskell",
				"constant.language.unit.haskell"
			],
			"settings": {
				"foreground": "#D69876"
			}
		},
		{
			"name": "Storage Modifier",
			"scope": [
				"storage.type.modifier"
			],
			"settings": {
				"foreground": "#A5346D"
			}
		},
		{
			"name": "Diff Addition",
			"scope": [
				"diff.addition"
			],
			"settings": {
				"foreground": "#449944"
			}
		},
		{
			"name": "Diff Deletion",
			"scope": [
				"diff.deletion"
			],
			"settings": {
				"foreground": "#993333"
			}
		}
	]
}
